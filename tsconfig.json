{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "outDir": "./output",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "noEmitHelpers": true,
    "declaration": true,
    "declarationDir": "./output/typings"
  },
  "exclude": [
    "demo",
    "dist",
    "node_modules",
    "typings",
    "src/libs"
  ]
}