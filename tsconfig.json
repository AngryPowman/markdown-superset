{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "outDir": "./output",
    "outFile": "./output/all.js",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false,
    "noEmitHelpers": true,
    "declaration": true,
    "pretty": true
    // "declarationDir": "./output/typings"
  },
  "exclude": [
    "demo",
    "dist",
    "output",
    "node_modules",
    "typings",
    "src/libs"
  ]
}